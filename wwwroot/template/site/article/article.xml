					$_asp item;
				</div>
			</div>
			]]>
		</box>
		<list_item use_language="true" use_elanat="true">
			<![CDATA[
			<div class="el_item el_comment_item el_parent_count_$_asp parent_count;">
				<div class="el_comment_title">$_asp comment_title;</div>
				<div class="el_user_avatar" style="background-image: url('$_elanat ::system site_path;client/image/user_avatar/$_asp user_guest_id;.png');"></div>
				<div class="el_details">
					<div class="el_user_name">$_asp comment_user_guest_name;</div>
					<div class="el_date_and_time">$_asp comment_date_and_time_send;</div>
