		<div class="el_action">
			<div class="el_send_email" title="$_lang send_email;" onclick="el_SendEmailContent($_db content_id;)"></div>
			<a href="$_elanat ::system site_path;page/print/$_db content_id;/" title="$_lang print;" target="_top" ><div class="el_print" src="$_elanat ::system site_path;client/image/print.png"></div></a>
		</div>
		$_asp content_icon;
            <div class="el_title">
                <h2>
					<a href='$_asp extra_content_url_value;'>
						$_db content_title;
					</a>
				</h2>
                <h3 class="el_content_meta">
					$_asp_lang author;
					<span>
					  <a href="$_elanat ::system site_path;page_content/public_user_info/name/$_db user_site_name;/" title="$_db user_site_name;" target="_top" >$_db user_site_name;</a>
					</span>
					<span>
					  $_db content_date_create;
					</span>
					<span>
					  $_db content_time_create;
					</span>
					$_asp_lang visit;
					<span>
					  $_db content_visit;
				  </span>
			  </h3>
            </div>
        </div>
        <div class='el_content_body'>
            <div class="el_content_body_top">
                <div class="el_content_body_top_first"></div>
                <div class="el_content_body_top_second">
                    <div class="el_category_name_box">
						$_asp_lang category; <a href="$_elanat ::system site_path;category/$_db category_name;/" title="$_asp category_local_name;" target="_top" >$_asp category_local_name;</a>
					</div>
                </div>
            </div>
            $_asp content_reply;
			$_asp content_avatar;
			<div class="el_paragraph">$_db content_text;</div>$_asp attachment;
			$_asp content_keywords;
                    <div class="el_content_body_bottom">
                        <div class="el_box">
                        <div class="el_rating_box">$_asp rating;</div>
                            <div class="el_comment_load">
                                <b class="el_release_content">
                                    <a href="javascript:void(0)" onclick="el_ViewComment('$_db content_id;')">$_lang comments; ($_db comment_count;)</a>
                                </b>
                            </div>
                        </div>
                        $_asp read_more;
                    </div>
        </div>
        $_asp comment;
    </div>
	]]>
