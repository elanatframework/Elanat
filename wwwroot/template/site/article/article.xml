	<go_to_admin><![CDATA[<a href="$_asp path;$_asp admin_path;" target="_top">$_lang admin;</a>]]></go_to_admin>
	<return_to_page><![CDATA[<a alt="$_asp path_alt;" href="$_asp path;">$_asp path_text;</a>]]></return_to_page>
</link>

<properties>
	<template_name>el hegemony</template_name>
	<![CDATA[board_color= white or black or gray or blue_gray or red or violet or yelow or pink or blue or green]]>
	<color>green</color>
	<![CDATA[board_width=px]]>
	<content_width>700</content_width>
	<![CDATA[board_height=px]]>
	<content_height>500</content_height>
</properties>

<content>
	<post use_language="true" use_elanat="true">
	<![CDATA[
    <div id="content_number_$_db content_id;" class="el_content_post">
        <div class="el_top">
		<div class="el_reply">
			<a href="javascript:void(0)" onclick="el_OpenContentReply($_db content_id;)">$_lang reply;</a>
		</div>
		<a href="#content_number_$_db content_id;"><span class="el_label_light el_content_number">#$_db content_id;</span></a>
		<div class="el_action">
			<div class="el_send_email" title="$_lang send_email;" onclick="el_SendEmailContent($_db content_id;)"></div>
			<a href="$_elanat ::system site_path;page/print/$_db content_id;/" title="$_lang print;" target="_top" ><div class="el_print" src="$_elanat ::system site_path;client/image/print.png"></div></a>
		</div>
		$_asp content_icon;
		<div class="el_title">
			<h2>
				<a href="$_asp extra_content_url_value;">
					$_db content_title;
				</a>
			</h2>
			<h3 class="el_content_meta">
				$_asp_lang author;
				<span>
				  <a href="$_elanat ::system site_path;page_content/public_user_info/name/$_db user_site_name;/" title="$_db user_site_name;" target="_top" >$_db user_site_name;</a>
			  </span>
			  <span>
				  $_db content_date_create;
			  </span>
			</h3>
		</div>
        </div>
        <div class='el_content_body'>
            <div class="el_content_body_top">
                <div class="el_content_body_top_first"></div>
                <div class="el_content_body_top_second">
                    <div class="el_category_name_box">
						$_asp_lang category; <a href="$_elanat ::system site_path;category/$_db category_name;/" title="$_asp category_local_name;" target="_top" >$_asp category_local_name;</a></div>
					</div>
            </div>
            $_asp content_reply;
			$_asp content_avatar;
			<div class="el_paragraph">$_db content_text;</div>$_asp attachment;
			$_asp content_keywords;
                    <div class="el_content_body_bottom">
                        <div class="el_box">
                          <div class="el_rating_box">$_asp rating;</div>
                          <div class="el_comment_load">
                              <b class="el_release_content">
                                  <a href="javascript:void(0)" onclick="el_ViewComment('$_db content_id;')">$_lang comments; ($_db comment_count;)</a>
                              </b>
                          </div>
                        </div>
                        $_asp read_more;
                    </div>
        </div>
        $_asp comment;
    </div>
	]]>
	</post>
	<alone_post use_language="true" use_elanat="true">
	<![CDATA[
    <div id="content_number_$_db content_id;" class="el_content_post">
        <div class="el_top">
        <div class="el_reply">
