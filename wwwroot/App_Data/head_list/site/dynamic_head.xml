<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<dynamic_head_root>
  <dynamic_head_list>
    
    <dynamic_style active="true">
    <![CDATA[
      <link id="link_Style" rel="stylesheet" type="text/css" href="$_asp site_path;client/style/site/$_asp style_physical_path;" />
    ]]>
    </dynamic_style>

    <dynamic_meta_right active="true">
    <![CDATA[
      <meta id="meta_Right" name="rights" content="$_asp meta_right;" />
    ]]>
    </dynamic_meta_right>

    <dynamic_meta_revisit_after active="true">
    <![CDATA[
      <meta id="meta_RevisitAfter" name="REVISIT-AFTER" content="$_asp meta_revisit_after;" />
    ]]>
    </dynamic_meta_revisit_after>

    <dynamic_meta_keywords active="true">
    <![CDATA[
      <meta id="meta_Keywords" name="keywords" content="$_asp meta_keywords;"/>
    ]]>
    </dynamic_meta_keywords>

    <dynamic_meta_description active="true">
    <![CDATA[
      <meta id="meta_Description" name="description" content="$_asp description;" />
    ]]>
    </dynamic_meta_description>

    <dynamic_meta_classification active="true">
    <![CDATA[
      <meta id="meta_Classification" name="classification" content="$_asp classification;" />
    ]]>
    </dynamic_meta_classification>

    <dynamic_meta_author active="true">
    <![CDATA[
      <meta id="meta_Author" name="author" content="$_asp author;" />
    ]]>
    </dynamic_meta_author>

    <dynamic_meta_copyright active="true">
    <![CDATA[
      <meta id="meta_Copyright" name="copyright" content="$_asp copyright;" />
    ]]>
    </dynamic_meta_copyright>

    <dynamic_meta_language active="true">
    <![CDATA[
      <meta id="meta_Language" http-equiv="language" content="$_asp language;" />
    ]]>
    </dynamic_meta_language>

    <dynamic_meta_robots active="true">
    <![CDATA[
      <meta id="meta_Robots" name="robots" content="$_asp robots;" />
    ]]>
    </dynamic_meta_robots>

    <dynamic_meta_application_name active="true">
    <![CDATA[
      <meta name="application-name" content="$_asp application_name;" />
    ]]>
    </dynamic_meta_application_name>

    <dynamic_meta_generator active="true">
    <![CDATA[
      <meta name="generator" content="$_asp generator;" />
    ]]>
    </dynamic_meta_generator>

    <dynamic_meta_javascript_inactive_refresh active="true">
    <![CDATA[
    <noscript>
			<meta http-equiv="refresh" content="0; url=$_asp site_path;?javascript_active=false" /><!-- path to action/javascript_active=false -->
		</noscript>
    ]]>
    </dynamic_meta_javascript_inactive_refresh>

    <dynamic_box_head active="true">
    <![CDATA[
      $_asp box_head;
    ]]>
    </dynamic_box_head>
 
</dynamic_head_list>
</dynamic_head_root>