	<page_load>
		<ajax>
			<![CDATA[
			<script>
			var xmlhttp = (window.XMLHttpRequest) ? xmlhttp = new XMLHttpRequest() : new ActiveXObject("Microsoft.XMLHTTP");

			xmlhttp.onreadystatechange = function()
			{
				if (xmlhttp.readyState == 4 && xmlhttp.status == 200)
				{
					document.write(xmlhttp.responseText);
					el_AppendJavaScriptTag(xmlhttp.responseText);
					el_AppendStyleTag(xmlhttp.responseText);
				}
			}
			xmlhttp.open("GET", '$_asp page_path;', false);
			xmlhttp.send();
			</script>
			]]>
		</ajax>
		<iframe>
		<![CDATA[
			<iframe id="iframe" name="iframe" class="el_iframe_load" scrolling="no" src="$_asp page_path;" onload="el_IframeAutoHeight(this)" frameborder="0"></iframe>
		]]>
		</iframe>
		<ajax_quick_load>
			<![CDATA[
				el_AjaxLoad("$_asp page_path;");
			]]>
		</ajax_quick_load>
		<iframe_quick_load>
		<![CDATA[
			<iframe id="iframe" name="iframe" class="el_iframe_load" scrolling="no" src="$_asp page_path;" onload="el_IframeAutoHeight(this)" frameborder="0"></iframe>
		]]>
		</iframe_quick_load>
		<iframe_create_html_for_part_page>
		<![CDATA[
			<iframe id="iframe" name="iframe" scrolling="no" src="$_asp site_path;action/create_html_for_part_page/?page_id=$_asp page_id;" onload="el_IframeAutoHeight(this)" frameborder="0"></iframe>
		]]>
		</iframe_create_html_for_part_page>
	</page_load>
	
